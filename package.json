{
  "name": "airbag-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Provide DB functionality",
  "scripts": {
    "lint": "eslint src test --fix",
    "test": "mocha --require @babel/register test/*.js",
    "build": "NODE_ENV=production babel gs_src --out-dir dist && npx webpack",
    "ln1": "npm link @airbag/sheet-helper",
    "ln2": "npm link @airbag/sheet-wrapper",
    "ln3": "npm link @airbag/kava",
    "relink": "npm run ln1 && npm run ln2 && npm run ln3",
    "deploy": "cd dist && clasp push && cd .."
  },
  "author": "Slavik",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-promise": "^4.0.1",
    "lodash-webpack-plugin": "^0.11.5",
    "mocha": "^6.0.2",
    "sinon": "^7.2.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-plugin-replace": "^1.2.0"
  },
  "dependencies": {
    "@airbag/kava": "^1.0.0",
    "@airbag/sheet-helper": "^1.0.0",
    "@airbag/sheet-wrapper": "^1.0.0",
    "@babel/polyfill": "^7.2.5",
    "@types/google-apps-script": "0.0.41",
    "lodash": "^4.17.11",
    "node-uuid": "^1.4.8",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "reselect": "^4.0.0"
  }
}
